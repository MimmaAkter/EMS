@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <div class="error-pagewrap">
        <div class="error-page-int">
            <div class="text-center m-b-md custom-login">
                <h3>PLEASE LOGIN TO SYSTEM</h3>
                @*<p>This is the best system ever!</p>*@
            </div>
            <div class="content-error">
                <div class="hpanel">
                    <div class="panel-body">
                        <form action="#" id="loginForm">
                            <div class="form-group">
                                <label class="control-label" for="username">Employee ID</label>
                                @*<input type="text" placeholder="example@gmail.com" title="Please enter you username" required="" value="" name="username" id="username" class="form-control">*@
                                @Html.EditorFor(model => model.EMPLOYEEID, new { htmlAttributes = new { @class = "form-control", @placeholder = "Employee ID", @type = "text", @name = "username" } })
                                @Html.ValidationMessageFor(model => model.EMPLOYEEID, String.Empty, new { @style = "color:red" })
                                <span class="help-block small">Your unique Employee Id to Login</span>
                            </div>
                            <div class="form-group">
                                <label class="control-label" for="password">Password</label>
                                @*<input type="password" title="Please enter your password" placeholder="******" required="" value="" name="password" id="password" class="form-control">*@
                                @Html.EditorFor(model => model.EMPPASSWORD, new { htmlAttributes = new { @class = "form-control", @placeholder = "******", @type = "password", @name = "password" } })
                                @Html.ValidationMessageFor(model => model.EMPPASSWORD, String.Empty, new { @style = "color:red" })
                                <span class="help-block small">Your strong password</span>
                            </div>
                            <div class="checkbox login-checkbox">
                                <label>
                                    <input type="checkbox" class="i-checks"> Remember me
                                </label>
                                <p class="help-block small">(if this is a private computer)</p>
                            </div>
                            <button class="btn btn-success btn-block loginbtn" onclick="Login()">Login</button>                            
                            <button class="btn btn-default btn-block" href="/Views/Shared/_Layout.cshtml">Cancel</button>                           
                        </form>
                    </div>
                </div>
            </div>
            <div class="text-center login-footer">
                <p>Copyright © 2021. Developed by Mimma Akter <a href="https://colorlib.com/wp/templates/">Dhaka University PGDIT 20</a></p>
            </div>
        </div>
    </div>
}
